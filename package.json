{
  "name": "griffon-toolkit-root",
  "private": true,
  "version": "0.3.0",
  "main": "dist/index.js",
  "dependencies": {
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/node": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "@types/ramda": "^0.27.46",
    "ajv": "^6.12.3",
    "babel-plugin-inline-json-import": "^0.3.2",
    "ctix": "^0.3.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsdoc": "^37.0.3",
    "fs-extra": "^10.0.0",
    "jest": "^27.3.1",
    "jest-junit": "^13.0.0",
    "jsdoc": "^3.6.7",
    "jsdoc-memberof-namespace": "^2.2.0",
    "json-schema-to-typescript": "^10.1.5",
    "lerna": "^4.0.0",
    "lodash": "^4.17.21",
    "ts-jest": "^27.0.7",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.4.4"
  },
  "scripts": {
    "coverage": "npm run test --coverage",
    "docs": "rm -rf docs; jsdoc -c .jsdoc.json --package ''",
    "postdocs": "mv -f docs-temp/griffon-toolkit-root/* docs; rm -rf docs-temp",
    "generate": "npm run generate-code && npm run docs",
    "generate-code": "npx babel-node ./scripts/generate.js && rm -rf tmp",
    "postinstall": "lerna bootstrap",
    "build:make-index": "lerna exec --parallel --ignore @adobe/griffon-toolkit 'npx ctix create ./tsconfig.json'",
    "build:files": "lerna exec --parallel 'tsc'",
    "build": "npm run build:make-index && npm run build:files",
    "lint": "eslint \"**/*.js\"",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/griffon-toolkit.git"
  },
  "author": "Adobe Systems",
  "license": "Apache-2.0",
  "jest": {
    "preset": "ts-jest/presets/js-with-babel-esm",
    "testEnvironment": "jsdom",
    "testMatch": [
      "**/test/**/?(*.)+(spec|test).(ts|js)"
    ]
  }
}
