{
  "$id": "http://griffon.adobe.com/schemas/receivePlaces",
  "shortDesc": "Receive Places Event",
  "allOf": [{ "$ref": "http://griffon.adobe.com/schemas/genericPlaces" }],
  "type": "object",
  "properties": {
    "payload": {
      "inherit": true,
      "type": "object",
      "properties": {
        "ACPExtensionEventData": {
          "inherit": true,
          "alias": "data",
          "type": "object",
          "properties": {
            "nearbypois": {
              "alias": "POIs",
              "description": "The resulting POIs",
              "match": true,
              "type": "array",
              "contains": {
                "$ref": "http://griffon.adobe.com/schemas/poi"
              }
            }
          },
          "required": [
            "nearybypois"
          ]
        },
        "ACPExtensionEventSource": {
          "inherit": true,
          "const": "com.adobe.eventsource.responsecontent"
        }
      },
      "required": [
        "ACPExtensionEventData"
      ]
    }
  }
}