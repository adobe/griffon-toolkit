{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["packages", "package.json", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-memberof-namespace"
  ],
  "templates": {
    "referenceTitle": "griffon-toolkit",
    "disableSort": false,
    "collapse": true
  },
  "opts": {
    "destination": "./docs-temp",
    "encoding": "utf8",
    "private": true,
    "recurse": true,
    "tutorials": "./tutorials",
    "template": "./jsdoc-template"
  }
}